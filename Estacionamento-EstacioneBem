--1.Criando tabela de dados 
create database estacinamento;
use estacinamento;


create table cliente (
id_cliente int primary key auto_increment
nome varchar (100)not null
telefone varchar (30)
email varchar (100)unique
endereco varchar(200)

);

create table veiculo (
id_veiculo int primary key auto_increment
placa varchar (10) unique not null	
modelo varchar (50) not null
cor varchar (50)
id_cliente int 
foreign key (id_cliente) references (id_cliente)on delete cascade
);

create table registro (
id_registro int primary key auto_increment
id_veiculo int	
entrada datetime not null
saida datetime
foreign key(id_veiculo) references (id_veiculo) on delete cascade
);
--inserir dados dos clientes
