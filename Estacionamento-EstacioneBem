--consultar tabela de clientes
  
select * from cliente 
select * from veiculo order by modelo asc;
select * from registro order by entrada asc limit  2;
select v.*,c.nome as nome_cliente from veiculo v 
join registro r on v.id_veiculo = r.id_veiculo
where r.saida is null;
--alterar tabela cliente
alter table  cliente add column data_nascimento date;
UPDATE clientes SET data_nascimento = '1981-06-04' WHERE id_cliente = 1;
UPDATE clientes SET data_nascimento = '2007-06-06' WHERE id_cliente = 2;
UPDATE clientes SET data_nascimento = '1978-02-22' WHERE id_cliente = 3;
UPDATE clientes SET data_nascimento = '1994-07-20' WHERE id_cliente = 4;
UPDATE clientes SET data_nascimento = '1979-03-06' WHERE id_cliente = 5;
UPDATE clientes SET data_nascimento = '1924-05-03' WHERE id_cliente = 6;
UPDATE clientes SET data_nascimento = '1977-09-27' WHERE id_cliente = 7;
UPDATE clientes SET data_nascimento = '2000-03-14' WHERE id_cliente = 8;
UPDATE clientes SET data_nascimento = '1988-11-15' WHERE id_cliente = 9;
UPDATE clientes SET data_nascimento = '1991-04-25' WHERE id_cliente = 10;
--deletar colunas
alter table cliente drop column endereco;
delete from cliente where id_cliente = 6
 

